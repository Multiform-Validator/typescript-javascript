{
	"compilerOptions": {
		"target": "ES2017",
		"module": "CommonJS",
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"strictNullChecks": true,
		"removeComments": true,
		"moduleResolution": "node",
	},
	"include": ["src/**/*", "index.ts"],
}
